  "dependencies": {
    "framer-motion": "^12.0.6",
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "standardized-audio-context": "^25.3.77"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webpack": "^5.28.5",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "uglify-js": "^3.17.4"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/tests/**/*.test.js"],
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.next/",
      "/tests/sdk-advanced.test.js",
      "/tests/sdk-webrtc.test.js",
      "/tests/sdk-session.test.js"
